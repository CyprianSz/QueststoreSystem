<div id="main-container">
    <div id="user-form-container">
        <form method="POST">
            <div class="user-form">
                <input type="text" name="rank" id="rank" value="{{ rank }}" required>
            </div>

            <div class="user-form">
                <input type="text" name="required-experience" id="required-experience" value="{{ requiredExperience }}" required>
            </div>

            <div class="user-form">
                <input type="text" name="description" id="description" value="{{ description }}" required>
            </div>

            <div id="user-form-button">
                <input type="submit" value="CREATE LEVEL">
            </div>
        </form>
    </div>
</div>
