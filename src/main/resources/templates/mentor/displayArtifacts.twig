<div id="main-container">

    <div id="teamsList">
        <table class="table">
            <thead>
            <tr>
                <th>name</th>
                <th>description</th>
                <th>type</th>
                <th>value</th>
            </tr>
            </thead>
            <tbody>
            {% for artifact in allArtifacts %}
                <tr>
                    <td>{{ artifact.Name }}</td>
                    <td>{{ artifact.description }}</td>
                    <td>{{ artifact.type }}</td>
                    <td>{{ artifact.value }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
</div>
